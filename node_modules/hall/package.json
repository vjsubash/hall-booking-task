{
  "name": "hall",
  "description": "Unified http router middleware",
  "version": "0.5.2",
  "repository": "rumkin/hall",
  "devDependencies": {
    "should": "~7.0.2",
    "mocha": "~2.2.5",
    "blanket": "~1.1.7"
  },
  "dependencies": {
    "route-parser": "0.0.5"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha test/**.spec.js",
    "cov": "node_modules/.bin/mocha test/**.spec.js -r blanket -R html-cov > tmp/coverage.html"
  },
  "main": "src/router.js",
  "config": {
    "blanket": {
      "pattern": [
        "src"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  }
}
